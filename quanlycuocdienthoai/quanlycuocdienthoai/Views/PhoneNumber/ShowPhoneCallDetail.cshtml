@model IEnumerable<quanlycuocdienthoai.Models.PhoneCallDetail>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            SĐT
        </th>
        <th>
            Thời gian bắt đầu
        </th>
        <th>
            Thời gian kết thúc
        </th>
        <th>
            Tổng tiền
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.PhoneNumberFKNavigation.PhoneNo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TimeStart)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TimeFinish)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SubTotal)
    </td>
</tr>
}

</table>